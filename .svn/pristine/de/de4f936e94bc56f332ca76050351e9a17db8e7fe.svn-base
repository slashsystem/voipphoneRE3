body {
	background-color: #ffffff;
	font-family:"Trebuchet MS", Arial, Helvetica, sans-serif;
	font-size: 10pt;
	font-weight: normal;
	color: #000000;
}

* { 
	margin: 0px; 
	padding: 0px; 
} 

p { 
	padding: 0px 0px 7pt 0px; 
} 

img { 
	border: none; 
} 

a {
	color: #11aaff;
	text-decoration: none;
}

#metanavigation, #language, #logininfo, #maintop, #navigation, #related-content, #title, #footer ul {
	display: none;
}

.button-left, .button-right, button {
	display: none;
}

#logo img {
	float: left;
	margin-left: 0px;
	margin-top: 0px;
	border: none;
}

#topnav {
	float: right;
}

h1 {
	font-size: 14pt;
	padding-bottom: 7pt;
}

h2 {
	font-size: 14pt;
	padding-bottom: 7pt;
	margin-top: 14pt;
}

h3 {
	font-size: 14pt;
	padding-bottom: 7pt;
}

h4 {
	font-size: 14pt;
	padding-bottom: 7pt;
}

h5 {
	font-size: 13pt;
	padding-bottom: 7pt;
}

h6 {
	font-size: 13pt;
	padding-bottom: 7pt;
}

ul { 
	list-style:none; 
} 


#main {
	clear: both;
}

#footer {
	clear: both;
	padding-top: 20pt;
	padding-left: 25px;
}


